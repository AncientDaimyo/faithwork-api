<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Product\Domain\Entity\Description" table="description">
        <id name="id" type="uuid" column="id"></id>
        <field name="print" type="string" column="print" length="255" nullable="true"/>
        <field name="density" type="string" column="density" length="255" nullable="true"/>
        <field name="compound" type="string" column="compound" length="255" nullable="true"/>
    </entity>
</doctrine-mapping>
